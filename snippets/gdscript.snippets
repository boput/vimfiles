snippet fu
	func ${1:name}(${2}):
		${3:pass}
snippet pr
	var ${1:property}${2} setget set_$1, get_$1
	func set_$1(value):
		$1 = value
	func get_$1():
		return $1
snippet p
	prints(${1})
snippet if
	if ${1:condition}:
		${2:pass}
snippet ife
	if ${1:condition}:
		${2:pass}
	else:
		${3:pass}
snippet for
	for ${1:var} in ${2:container}:
		${3:pass}
