			"skinparam backgroundcolor transparent",
			"left to right direction",
			"",
			"actor customer",
			"actor clerk",
			"rectangle checkout {",
			"\tcustomer -- (checkout)",
			"\t(checkout) .> (payment) : <<include>>",
			"\t(help) .> (checkout) : <<extends>>",
			"\t(checkout) -- clerk",
			"}",
			"$0",
			"...."
		],
		"description": ""
