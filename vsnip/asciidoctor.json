	"Plantuml Diagram": {
		"prefix": ["diag", "diagram", "uml"],
		"body": [
			"[plantuml, ${1:diagram-name}, ${2:svg}]",
			"....",
			"skinparam monochrome false",
			"skinparam shadowing true",
			"skinparam dpi 100",
			"skinparam backgroundcolor transparent",
			"",
			"$0",
			"...."
		],
		"description": ""
	},
	
	"Entity Relation Diagram": {
		"prefix": ["erd"],
		"body": [
			"[plantuml, ${1:erd-name}, ${2:svg}]",
			"....",
			"skinparam monochrome false",
			"skinparam shadowing true",
			"skinparam dpi 100",
			"skinparam backgroundcolor transparent",
			"skinparam classarrowFontSize 8",
			"",
			"!define table(x) class x << (T,#ede7aa) >>",
			"!define view(x) class x << (V,lightblue) >>",
			"!define f(n,t) {field}n <color:gray>: t</color>",
			"!define string(n,size) {field}n <color:gray>: varchar(size)</color>",
			"",
			"hide methods",
			"hide stereotypes",
			"",
			"table(mdc.Party) {",
			"\tf(**source_id**, **int**)",
			"\tf(master_id, int)",
			"\t..",
			"\tstring(firstname, 100)",
			"\tstring(lastname, 100)",
			"\tstring(gender, 1)",
			"}",
			"",
			"table(mdc.Contact) {",
			"\tf(**source_id**, **int**)",
			"\tf(master_id, int)",
			"\t..",
			"\tf(//party_master_id//, //int//)",
			"\tf(type, int)",
			"\tstring(value, 100)",
			"}",
			"",
			"table(mdc.Document) {",
			"\tf(**source_id**, **int**)",
			"\tf(master_id, int)",
			"\t..",
			"\tf(//party_master_id//, //int//)",
			"\tstring(type, 2)",
			"\tstring(number, 100)",
			"}",
			"",
			"table(mdc.Address) {",
			"\tf(**source_id**, **int**)",
			"\tf(master_id, int)",
			"\t..",
			"\tf(//party_master_id//, //int//)",
			"\tstring(country, 3)",
			"\tstring(city, 50)",
			"\tstring(street, 100)",
			"}",
			"",
			"mdc.Party \"1\" --{ \"0..*\" mdc.Address: Party has addresses",
			"mdc.Party \"1\" --{ \"0..*\" mdc.Document: Party has documents",
			"mdc.Party \"1\" --{ \"0..*\" mdc.Contact: Party has contacts",
			"...."
		],
		"description": ""
	},

