{

  "function": {
    "prefix": ["fu"],
    "body": [
      "func! ${1:MyFunc}($2) ${3:abort}",
      "\t$0",
      "endfunc"
    ],
    "description": ""
  },

  "let": {
    "prefix": ["let"],
    "body": [
      "let ${1:l}:${2:variable} = ${0:value}"
    ],
    "description": ""
  },

  "set": {
    "prefix": ["set"],
    "body": [
      "set ${1:option}=${0:value}"
    ],
    "description": ""
  },

  "augroup": {
	  "prefix": ["au"],
	  "body": [
		  "augroup ${1:augroup_name} | au!",
		  "	au ${2:BufRead,BufNewFile} ${3:*.ext} ${0}",
		  "augroup end"
	  ],
	  "description": ""
  }

}
