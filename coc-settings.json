{
    "diagnostic.enableSign": false,
    "coc.source.around.firstMatch": false,
    "coc.source.buffer.firstMatch": false,

    "python.jediEnabled": true,

    "languageserver": {
        "godot": {
            "host": "127.0.0.1",
            "filetypes": ["gdscript"],
            "port": 6008
        },
        "golang": {
            "command": "gopls",
            "rootPatterns": ["go.mod"],
            "filetypes": ["go"]
        }
    }
}
