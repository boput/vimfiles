priority -60

global !p
from vimsnippets import foldmarker, make_box, get_comment_format
endglobal

###################
#  Unicode chars  #
###################
snippet * "Unicode small circle"
• $0
endsnippet

snippet -> "Unicode arrow"
→ $0
endsnippet

snippet :heart: "Unicode heart"
❤️ $0
endsnippet

snippet [] "Unicode blank box"
☐ $0
endsnippet
 	
snippet [x] "Unicode check box"
☑ $0
endsnippet
 
snippet ^ "Unicode check mark"
✓ $0
endsnippet

snippet ^^ "Unicode heavy check mark"
✔ $0
endsnippet

###########
#  Boxes  #
###########
snippet box "A nice box with the current comment symbol" b
`!p
box = make_box(len(t[1]))
snip.rv = box[0]
snip += box[1]
`${1:${VISUAL:content}}`!p
box = make_box(len(t[1]))
snip.rv = box[2]
snip += box[3]`
$0
endsnippet

snippet bbox "A nice box over the full width" b
`!p
if not snip.c:
	width = int(vim.eval("&textwidth - (virtcol('.') == 1 ? 0 : virtcol('.'))")) or 71
box = make_box(len(t[1]), width)
snip.rv = box[0]
snip += box[1]
`${1:${VISUAL:content}}`!p
box = make_box(len(t[1]), width)
snip.rv = box[2]
snip += box[3]`
$0
endsnippet

##########
#  Misc  #
##########

snippet todo "TODO comment" w
`!p snip.rv=get_comment_format()[0]`${2:TODO}: ${0:${VISUAL}} <`!v strftime("%Y-%m-%d %H:%M")`>`!p snip.rv=(get_comment_format()[2] if get_comment_format()[0] != get_comment_format()[2] else '')`
endsnippet

snippet uuid "Random UUID" w
`!p if not snip.c: import uuid; snip.rv = uuid.uuid4()`
endsnippet

#########
# DATES #
#########
snippet date "YYYY-MM-DD" w
`!v strftime("%Y-%m-%d")`
endsnippet

snippet ddate "Month DD, YYYY" w
`!v strftime("%b %d, %Y")`
endsnippet

snippet diso "ISO format datetime" w
`!v strftime("%Y-%m-%d %H:%M:%S%z")`
endsnippet

snippet time "hh:mm" w
`!v strftime("%H:%M")`
endsnippet

snippet datetime "YYYY-MM-DD hh:mm" w
`!v strftime("%Y-%m-%d %H:%M")`
endsnippet

